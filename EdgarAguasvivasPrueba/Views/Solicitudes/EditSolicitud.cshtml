@model Solicitud

@{
    Layout = "_Layout";   
}

<div asp-validation-summary="ModelOnly" class="text-danger"></div>
<form class="m-1 p-1" method="post">
    <div class="form-group">
        <label asp-for="Id"></label>
        <input asp-for="Id" type="text" readonly class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Personas"></label>
        <select asp-for="Personas">           
        </select>
        <span asp-validation-for="Personas" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Estados"></label>
        <select asp-for="Estados">           
        </select>
        <span asp-validation-for="Estados" class="text-danger"></span>
    </div>
    
    <button type="submit" class="btn btn-primary">Actualizar</button>
</form> 